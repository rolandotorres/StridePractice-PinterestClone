

<p id="notice"><%= notice %></p>


  <div class="panel-heading"></div>
   
<div class="panel-body">
      <% @pins.each do |pin| %>
        <%= image_tag pin.image.url( :medium) %>
        <strong>Description:</strong><%= pin.description %><br/>
        <%= link_to 'Show', pin, class: 'btn btn-info' %>&nbsp;
        <% if pin.user == current_user %>
        <%= link_to 'Edit', edit_pin_path(pin),class: 'btn btn-warning' %>&nbsp;
        <%= link_to 'Destroy', pin, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %>
        <% end %>
        <br/>
    <% end %>
    
  <!--#% if user_signed_in? %>
  <#% javascript_include_tag :defaults %>
  <#%= link_to 'New Pin', new_pin_path, class: 'btn btn-success btn-lg' %>
  <#% end %-->
</div>
